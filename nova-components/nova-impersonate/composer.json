{
    "name": "trosthelden/nova-impersonate",
    "description": "A Laravel Nova field allows you to authenticate as your users.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "trosthelden\\NovaImpersonate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "trosthelden\\NovaImpersonate\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
